{
  "name": "sendit-api",
  "version": "1.0.0",
  "description": "this is just the api for the rest of the project",
  "main": "server.js",
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./api/server.js",
    "dev": "nodemon ./api/server.js",
    "build": "webpack -w",
    "report": "nyc report",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "cross-env NODE_ENV=test nyc node_modules/.bin/jasmine",
    "test5": "babel-node node_modules/.bin/jasmine",
    "test6": "istanbul cover --include-all-sources jasmine-node test",
    "ijcover": "istanbul cover --include-all-sources babel-node node_modules/.bin/jasmine"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karekkhaleb/SendIT.git"
  },
  "author": "Caleb",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/karekkhaleb/SendIT/issues"
  },
  "homepage": "https://github.com/karekkhaleb/SendIT#readme",
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/register": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "istanbul": "^0.4.5",
    "jasmine": "^3.3.0",
    "nyc": "^13.1.0",
    "request": "^2.88.0",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "jasmine-es6": "^0.4.3"
  }
}
